{
  "manifest_version": 2,
  "name": "My frist Extension",
  "version": "1.0.0",

  "permissions": [
    "http://*/"
  ],

  "content_scripts": [
  {
    "matches": [
      "<all_urls>"
    ],
    "js": ["jquery-2.2.4.min.js", "content.js"],
    "background": {
        "scripts": ["background.js"]
    }
  }
]
}
